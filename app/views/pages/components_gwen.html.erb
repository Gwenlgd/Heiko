      </div>
      <div class="carousel-item">
        <h2>Moods</h2>
        <div class="category-ms moods">
          <div class="card-ms">
            <div class="square">
              <%= inline_svg_tag("superhappy.svg", class:"card-ms-icon" )  %>
            </div>
            <p>Super happy</p>
          </div>
          <div class="card-ms">
            <div class="square">
